<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme, dependent on API level.
         This theme is replaced by AppBaseTheme from res/values-vXX/styles.xml on newer devices. -->
    <style name="drive_AppBaseTheme" parent="@style/Theme.AppCompat.Light">
        <!-- Theme customizations available in newer API levels can go in res/values-vXX/styles.xml,
             while customizations related to backward-compatibility can go here. -->
    </style>

    <!-- Application theme. -->
    <style name="drive_AppTheme" parent="drive_AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>
